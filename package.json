{
  "name": "create-react-prime",
  "version": "1.3.2",
  "description": "Installs the latest version of React-Prime boilerplate.",
  "main": "src/index.js",
  "bin": {
    "create-react-prime": "./src/index.js"
  },
  "scripts": {
    "lint": "eslint --ext .ts --ext .tsx src/",
    "clean": "rm -rf dist/",
    "start": "node -r ./tsconfig-paths-bootstrap.js dist/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly -r tsconfig-paths/register src/index.ts",
    "build": "npm run clean && tsc",
    "test": "mocha",
    "prepublishOnly": "npm run lint && npm run test",
    "pub:patch": "npm version patch && npm publish",
    "pub:minor": "npm version minor && npm publish",
    "pub:major": "npm version major && npm publish"
  },
  "dependencies": {
    "commander": "5.0.0",
    "progress-estimator": "0.2.2",
    "tsconfig-paths": "3.9.0"
  },
  "keywords": [
    "boilerplate",
    "install",
    "react"
  ],
  "author": "Sander Vispoel <contact@sandervispoel.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.2",
    "mocha": "7.1.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.9.1-rc",
    "typescript-eslint-parser": "22.0.0"
  }
}
